{% extends "SUHGestionBundle:AffichageEtudiants:base.html.twig" %}

{% block title %} {{parent()}} - Ajout d'un étudiant {% endblock %}

{% block body %}


<body>

    <form id="formulaire">
        <!-- Première étape : -->
        <h3>Identité de l'étudiant</h3>
        <section data-step="0" >
            <div class="col-md-12">
                <div class="col-md-3 form-group"  style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="nom">Nom :</label></br>
                    <input id="nom" type="text" class="form-control" value="{% if(info) %}{{info.listEtudiantInformations[0].etudiantInformations.nom}}{% endif %}" placeholder="Nom" pattern="^([a-zA-Z'()àâéèêôùûçÀÂÉÈÔÙÛÇ\s-]{1,30})$" title="Veuillez saisir un nom valide." required>
                </div>
            
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="prenom">Prénom :</label></br>
                    <input id="prenom" type="text" name="prenom" class="form-control" value="{% if(info) %}{{info.listEtudiantInformations[0].etudiantInformations.prenom}}{% endif %}" placeholder="Prenom" pattern="^([a-zA-Z'()àâéèêôùûçÀÂÉÈÔÙÛÇ\s-]{1,30})$" title="Veuillez saisir un prénom valide" required>
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label>Parite :</label></br>
                    <input style="display:inline-block;" id="pariteM" type="radio" 
                    {% if(info) %}
                        {% if (info.listEtudiantInformations[0].etudiantInformations.parite) == 'M' %} 
                        checked
                        {% endif %}
                    {% endif %}
                    name="parite"> masculin
                    <input style="display:inline-block;margin-left:40px;" id="pariteF" type="radio" 
                    {% if(info) %}
                        {% if (info.listEtudiantInformations[0].etudiantInformations.parite) == 'F' %} 
                        checked
                        {% endif %}
                    {% endif %}
                    name="parite"> feminin
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="dateNaissance">Date de naissance :</label></br>
                    <input id="dateNaissance" name="dateNaissance" class="form-control" value="{% if(info) %}{{info.dateNaissance|date("d/m/Y")}}{% endif %}" pattern="^[0-9]{2}[/]{1}[0-9]{2}[/]{1}[0-9]{4}$" title="Veuillez saisir une date de naissance valide au format JJ/MM/AAAA." placeholder="JJ/MM/AAAA">
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">

                    <label for="premiereInscription">Première Inscription :</label></br>
                    <input id="premiereInscription" name="premiereInscription" class="form-control" value="{% if(info) %}{{info.premiereInscription}} {% endif %}" pattern="^[0-9]{4}[-]{1}[0-9]{4}$" title="Veuillez saisir une année universitaire valide au format AAAA-AAAA." placeholder="AAAA-AAAA">
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">

                    <label for="numeroEtudiant">Numero étudiant :</label></br>
                    <input id="numeroEtudiant" type="text" class="form-control" value="{% if(info) %}{{info.numeroEtudiant}}{% endif %}" placeholder="Numéro étudiant">
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label>Qhandi :</label></br>
                    <input id="qhandi" class="form-control" placeholder="qhandi" type="text"
                    value="{% if(info) %}{{info.listEtudiantHandicape[0].etudiantHandicape.qhandi}}{% endif %}">
                </div>
            </div>
            <div class="col-md-12">
                <div id="tousLesHandicap" class="col-md-10 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label>Type d'handicap :</label></br>
                        <input id="TCOG" type="checkbox" 
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TCOG' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}> tcog
                        <input style="margin-left:40px" id="TPSY" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TPSY' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > tpsy
                        <input style="margin-left:40px" id="TAPPR" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TAPPR' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > tappr
                        <input style="margin-left:40px" id="TM" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TM' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > tm
                        <input style="margin-left:40px" id="TSI" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TSI' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > tsi
                        <input style="margin-left:40px" id="CEC" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='CEC' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > cec
                        <input style="margin-left:40px" id="TV" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TV' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > tv
                        <input style="margin-left:40px" id="SUR" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='SUR' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > sur
                        <input style="margin-left:40px" id="TA" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='TA' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > ta
                        <input style="margin-left:40px" id="POLY" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='POLY' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > poly
                        <input style="margin-left:40px" id="IT" type="checkbox"
                        {% if(info) %}
                            {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                                {% if handi.nomHandicap=='IT' %} 
                                    checked
                                {% endif %}
                            {% endfor %}
                        {% endif %}
                        > it
                    <input style="margin-left:40px" id="autreHandicap" type="checkbox"
                    {% if(info) %}
                        {% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %} 
                            {% if handi.nomHandicap!='TCOG' and handi.nomHandicap!='TPSY' and handi.nomHandicap!='TAPPR' and handi.nomHandicap!='TM' and handi.nomHandicap!='TSI' and handi.nomHandicap!='CEC' and handi.nomHandicap!='TV' and handi.nomHandicap!='SUR' and handi.nomHandicap!='TA' and handi.nomHandicap!='POLY' and handi.nomHandicap!='IT' and handi.nomHandicap!=null %} 
                                checked
                            {% endif %}
                        {% endfor %}
                    {% endif %}
                    > autre
                    <input id="textHandicap" type="text" class="form-control" value="{% if(info) %}{% for handi in info.listEtudiantHandicape[0].etudiantHandicape.handicap %}{% if handi.nomHandicap!='TCOG' and handi.nomHandicap!='TPSY' and handi.nomHandicap!='TAPPR' and handi.nomHandicap!='TM' and handi.nomHandicap!='TSI' and handi.nomHandicap!='CEC' and handi.nomHandicap!='TV' and handi.nomHandicap!='SUR' and handi.nomHandicap!='TA' and handi.nomHandicap!='POLY' and handi.nomHandicap!='IT' and handi.nomHandicap!=null %}{{handi.nomHandicap}}{% endif %}{% endfor %}{% endif %}">
                </div>
            </div>
        </section>

            <!-- Deuxième étape : -->

        <h3>Coordonnées de l'étudiant</h3>
        <section id="sectionCoordonnees" data-step="1">
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="mailInstitutionnel">Email institutionnel :</label></br>
                    <!-- pattern="^[^\W][a-zA-Z0-9_]+(\.[a-zA-Z0-9_]+)*\@[a-zA-Z0-9_]+(\.[a-zA-Z0-9_]+)*\.[a-zA-Z]{2,4}$" -->
                    <input id="mailInstitutionnel" type="email" class="form-control" placeholder="Email Institutionnel" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.mailInstitutionnel}}{% endif %}" title="Veuillez fournir une adresse électronique valide.">
                </div>
           
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="mailPerso">Email Personnel :</label></br>
                    <!-- pattern="^[^\W][a-zA-Z0-9_]+(\.[a-zA-Z0-9_]+)*\@[a-zA-Z0-9_]+(\.[a-zA-Z0-9_]+)*\.[a-zA-Z]{2,4}$"  -->
                    <input id="mailPerso" name="email1" type="email" class="form-control" placeholder="Email Personnel" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.mailPerso}}{% endif %}" title="Veuillez fournir une adresse électronique valide.">
                </div>
            
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="mailParents">Email Parents :</label></br>
                    <!-- pattern="^[^\W][a-zA-Z0-9_]+(\.[a-zA-Z0-9_]+)*\@[a-zA-Z0-9_]+(\.[a-zA-Z0-9_]+)*\.[a-zA-Z]{2,4}$"  -->
                    <input id="mailParents" name="email2" type="email" class="form-control" placeholder="Email Parents" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.mailParents}}{% endif %}" title="Veuillez fournir une adresse électronique valide.">
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="adresseEtudiante">Adresse étudiante :</label>
                    <input id="adresseEtudiante" class="form-control" placeholder="Adresse Etudiante" type="text" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.adresseEtudiante}}{% endif %}">
                </div>
            
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="adresseFamiliale">Adresse familiale :</label></br>
                    <input id="adresseFamiliale" class="form-control" placeholder="Adresse Familiale" type="text" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.adresseFamiliale}}{% endif %}">
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="telephonePerso">Téléphone Personnel :</label></br>
                    <input id="telephonePerso" name="telephonePerso" class="form-control" placeholder="Telephone personnel" type="tel" pattern="^(0|\+33)[1-9]([-. ]?[0-9]{2}){4}$" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.telephonePerso}}{% endif %}" title="Veuillez entrer un numéro de téléphone valide.">
                </div>
            
                <div class="col-md-3 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;margin : 3px; padding : 5px;">
                    <label for="telephoneParents">Téléphone Parents :</label></br>
                    <input id="telephoneParents" class="form-control" placeholder="Telephone parents" type="tel" name="telephone" pattern="^(0|\+33)[1-9]([-. ]?[0-9]{2}){4}$" value="{% if (info) %}{{info.listEtudiantInformations[0].etudiantInformations.telephoneParents}}{% endif %}" title="Veuillez entre un numéro de téléphone valide">
                </div>
            </div>
        </section>

        <!-- Troisième étape : -->

        <h3>Formation</h3>
        <section data-step="2" style="margin-left:auto;margin-right:auto;width:118%;">
            
            <div class="col-md-12">
                <div class="col-md-5 form-group" style="border-width:1px;border-style:solid;border-color:black; border-radius:10px;padding:15px;">
                    <label style="text-decoration : underline;">Formation n°1 :</label></br></br>

                    <label>Etablissement :</label>
                        <table>
                            <tr>
                                <td class="tableEtablissement">
                                <input style="display:inline-block;" id="UBP_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) == 'ubp' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > ubp
                                </td>
                                <td class="tableEtablissement">
                                <input style="display:inline-block; width:20px;margin-left:30px;" id="UDA_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) == 'uda' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > uda
                                </td>
                                <td class="tableEtablissement">
                                <input style="display:inline-block;width:20px;margin-left:30px;" id="UCA_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) == 'uca' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > uca
                                </td>
                                <td class="tableEtablissement">
                                <input style="display:inline-block;width:20px;margin-left:30px;" id="SIGMA_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) == 'sigma' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > sigma
                                </td>
                            </tr>
                            <tr>
                                <td class="tableEtablissement">
                                <input style="display:inline-block;" id="VETAGROSUP_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) == 'vetagrosup' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > vetagrosup
                                </td>
                            
                            
                                <td class="tableEtablissement">
                                <input style="display:inline-block;width:20px;margin-left:30px;" id="ENSACF_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}% if info.listEtudiantFormation[0].formation.etablissement|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) == 'ensacf' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > ensacf
                                </td>
                            
                            
                                <td class="tableEtablissement">
                                <input style="display:inline-block;width:20px;margin-left:30px;" id="autreEtablissement_1" type="radio" name="etablissement_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.etablissement) != 'ubp' and (info.listEtudiantFormation[0].formation.etablissement) != 'uda' and (info.listEtudiantFormation[0].formation.etablissement) != 'uca' and (info.listEtudiantFormation[0].formation.etablissement) != 'sigma' and (info.listEtudiantFormation[0].formation.etablissement) != 'vetagrosup' and (info.listEtudiantFormation[0].formation.etablissement) != 'ensacf' and (info.listEtudiantFormation[0].formation.etablissement) !=null %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > autre</br>
                                </td>
                            </tr>
                        </table>
                                
                                <input id="textAutreEtablissement_1" style="vertical-align:middle;" type="text" class="form-control" value="{% if(info) %}{% if info.listEtudiantFormation|length != 0 %}{% if (info.listEtudiantFormation[0].formation.etablissement) != 'ubp' and (info.listEtudiantFormation[0].formation.etablissement) != 'uda' and (info.listEtudiantFormation[0].formation.etablissement) != 'uca' and (info.listEtudiantFormation[0].formation.etablissement) != 'sigma' and (info.listEtudiantFormation[0].formation.etablissement) != 'vetagrosup' and (info.listEtudiantFormation[0].formation.etablissement) != 'ensacf' and (info.listEtudiantFormation[0].formation.etablissement) !=null %}{{info.listEtudiantFormation[0].formation.etablissement}}{% endif %}{% endif %}{% endif %}"> </br>
                                
                            
                        

                    <label for="composante">Composante :</label></br>
                    <input id="composante_A" type="text" placeholder="Composante 1" class="form-control" value="{% if (info) %}{% if info.listEtudiantFormation|length != 0 %}{{info.listEtudiantFormation[0].formation.composante}}{% endif %}{% endif %}"></br>

                    <label>Diplome :</label></br>
                        <table>
                            <tr>
                                <td class="tableDiplome">
                                <input style="display:inline-block;" id="licence_1" type="radio" name="diplome_1" 
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'licence' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > licence
                                </td>
                                <td class="tableDiplome">
                                <input style="display:inline-block;margin-left:20px;" id="licencePro_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'licence Pro' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > licence pro
                                </td>
                                <td class="tableDiplome">
                                <input style="display:inline-block;margin-left:20px;" id="master_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'master' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > master
                                </td>
                                <td class="tableDiplome">
                                <input style="display:inline-block;margin-left:20px;" id="doctorat_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'doctorat' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > doctorat
                                </td>
                            </tr>
                            <tr>
                                <td class="tableDiplome">
                                <input style="display:inline-block;" id="dut_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'dut' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > dut
                                </td>
                                <td class="tableDiplome">
                                <input style="display:inline-block;margin-left:20px;" id="concours_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'concours' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > concours
                                </td>
                                <td class="tableDiplome">
                                <input style="display:inline-block;margin-left:20px;" id="daeu_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) == 'daeu' %}
                                            checked
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                > daeu
                                </td>
                                <td class="tableDiplome">
                                <input style="display:inline-block;margin-left:20px;" id="autreDiplome_1" type="radio" name="diplome_1"
                                {% if(info) %}
                                    {% if info.listEtudiantFormation|length != 0 %}
                                        {% if (info.listEtudiantFormation[0].formation.diplome) != 'licence' and (info.listEtudiantFormation[0].formation.diplome) != 'licence Pro' and (info.listEtudiantFormation[0].formation.diplome) != 'master' and (info.listEtudiantFormation[0].formation.diplome) != 'doctorat' and (info.listEtudiantFormation[0].formation.diplome) != 'concours' and (info.listEtudiantFormation[0].formation.diplome) != 'dut' and (info.listEtudiantFormation[0].formation.diplome) != 'daeu' and (info.listEtudiantFormation[0].formation.diplome) != null %}
                                            checked
                                        {% endif %}
