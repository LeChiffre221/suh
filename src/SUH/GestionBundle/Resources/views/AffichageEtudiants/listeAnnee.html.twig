<select id="selectYear" class="form-control" style="margin-left:10px; margin-top:10px;">


    {% if app.session.get('filter')['year'] is defined %}
        {% set filter = app.session.get('filter') %}
        {% set year = filter['year'] %}

    {% else %}
        {% set year = 0 %}
    {% endif %}

    {% for y in listYears %}

        {% if y.anneeUniversitaire == year %}
            <option value="{{ y.anneeUniversitaire }}" selected>{{ y.anneeUniversitaire }}</option>

        {% else %}
            <option value="{{ y.anneeUniversitaire }}">{{ y.anneeUniversitaire }}</option>
        {% endif %}

    {% endfor %}

</select>